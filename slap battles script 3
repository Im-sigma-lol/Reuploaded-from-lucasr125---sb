Tab2:AddToggle({Name = "Rojo Spam (WORKS WITH ALL GLOVES)",Default = false,Callback = function(Value)
if Person == nil then
Person = game.Players.LocalPlayer.Name
end
_G.RojoSpam = Value
while _G.RojoSpam do
game:GetService("ReplicatedStorage"):WaitForChild("RojoAbility"):FireServer("Release", {game.Players[Person].Character.HumanoidRootPart.CFrame})
task.wait()
end
end})

Tab2:AddTextbox({Name = "Make person use rojo spam",Default = "Username",TextDisappear = false,Callback = function(Value)
if Value == "Me" or Value == "me" or Value == "Username" or Value == "" then
Person = game.Players.LocalPlayer.Name
else
Person = Value
end
end})

